int __cdecl o(int a1, const char **a2, const char **a3)
{
  system("/bin/sh");
  _exit(1);
  return n(a1, a2, a3);
}

int __cdecl n(int a1, const char **a2, const char **a3)
{
  char input[520];

  fgets(input, 512, stdin);
  printf(input);
  exit(1);
  return main(a1, a2, a3);
}

int main(int argc, const char **argv, const char **envp)
{
  return n();
}